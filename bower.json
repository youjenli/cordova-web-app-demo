{
  "name": "Html5Application",
  "description": "cordova crosswalk application demo",
  "version": "1.0.0",
  "main": "src/index.html",
  "ignore": [
    ".jshintrc",
    "**/*.txt",
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "keywords": [
    "cordova",
    "crosswalk"
  ],
  "dependencies": {
    "jquery": "1.9.1",
    "backbone": "1.1.2",
    "requirejs": "2.1.15",
    "cordova-promise-fs": "~0.13.0",
    "jquery-mobile-for-synnex": "https://github.com/youjenli/jquery-mobile-for-synnex.git#1.0.1",
    "jquery-i18next-plugin-for-synnex": "https://github.com/youjenli/jquery-i18next-plugin-for-synnex.git#1.0.3",
    "domReady": "requirejs-domready#~2.0.1"
  },
  "overrides": {
    "jquery": {
      "main": "./jquery.js"
    },
    "jquery-mobile-for-synnex": {
      "main": [
		"./images/icons-png/*",
		"./images/ajax-loader.gif",
		"./jquery-mobile-theme-for-synnex.css",
		"./jquery.mobile.icons.css",
		"./jquery.mobile.structure.css",
		"./jquery.mobile.js"
		]
    }
  }
}
