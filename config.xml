<?xml version='1.0' encoding='utf-8'?>
<widget id="com.synnex.Html5Application" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Html5Application</name>
    <description>
		A sample Apache Cordova application that responds to the
		deviceready
		event.
	</description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
		Apache Cordova Team
	</author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" version="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <feature name="Console">
        <param name="wp-package" value="DebugConsole" />
        <param name="id" value="cordova-plugin-console" />
    </feature>
    <feature name="File">
        <param name="wp-package" value="File" />
        <param name="id" value="cordova-plugin-file" />
    </feature>
    <feature name="FileTransfer">
        <param name="android-package" value="org.apache.cordova.filetransfer.FileTransfer" />
        <param name="wp-package" value="FileTransfer" />
    </feature>
    <feature name="File Transfer">
        <param name="id" value="cordova-plugin-file-transfer" />
    </feature>
    <feature name="Geolocation">
        <param name="android-package" value="org.apache.cordova.geolocation.Geolocation" />
        <param name="id" value="cordova-plugin-geolocation" />
    </feature>
    <feature name="NetworkStatus">
        <param name="android-package" value="org.apache.cordova.networkinformation.NetworkManager" />
        <param name="wp-package" value="NetworkStatus" />
    </feature>
    <feature name="Network Information">
        <param name="id" value="cordova-plugin-network-information" />
    </feature>
    <feature name="SplashScreen">
        <param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen" />
        <param name="onload" value="true" />
        <param name="wp-package" value="SplashScreen" />
    </feature>
    <feature name="Splashscreen">
        <param name="id" value="cordova-plugin-splashscreen" />
    </feature>
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
        <param name="id" value="cordova-plugin-whitelist" />
    </feature>
    <preference name="AndroidPersistentFileLocation" value="Internal" />
    <preference name="xwalkVersion" value="15+" />
    <preference name="xwalkCommandLine" value="--disable-pull-to-refresh-effect" />
    <preference name="xwalkMode" value="embedded" />
    <preference name="xwalkMultipleApk" value="true" />
</widget>
